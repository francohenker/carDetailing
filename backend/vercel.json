    {
        "version": 2,
        "builds": [
            {
                "src": "src/main.ts",
                "use": "@vercel/node"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "dist/apps/carDetailing/main.ts",
                "methods": [
                    "GET",
                    "POST",
                    "PUT",
                    "DELETE",
                    "OPTIONS"
                ]
            }
        ],
        "outputDirectory": "dist",
        "buildCommand": "npm run vercel:build"
    }